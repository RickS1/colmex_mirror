<%= render 'principal/barra' %>

<div class="container-fluid" id="resto-cuerpo">
	<div id="cuerpo">
		<% @resultados.each do |r| %>
			<% if r.is_a?(Curso) %>
				<%= "Curso: " + r.titulo %>
			<% elsif r.is_a?(Descubre) %>
				<%= "Descubre: " + r.titulo %>
			<% else %>
				<%= "Evento (" + r["tipoActividad"] + ": " + r["tituloActividad"] + ": " + r["subtituloActividad"] %>
			<% end %>
		<br>
		<% end %>
	</div>

<%= render 'principal/footer' %>
